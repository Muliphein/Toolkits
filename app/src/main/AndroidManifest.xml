<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="io.github.muliphein.toolkits">

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_fire"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_fire"
        android:supportsRtl="true"
        android:theme="@style/Theme.Toolkits">
        <activity
            android:name=".MainActivity"
            android:exported="true"
            android:label="@string/app_name"
            android:theme="@style/Theme.Toolkits.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AlbumReadActivity"
            android:exported="true"
            android:label="@string/activity_album_read"
            android:theme="@style/Theme.Toolkits.NoActionBar">
        </activity>
    </application>

</manifest>